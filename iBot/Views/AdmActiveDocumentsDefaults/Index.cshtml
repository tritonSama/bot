@model IEnumerable<iBot.Models.IbotDevelopment.AdmActiveDocumentsDefault>

@{
    ViewData["Title"] = "ActiveAdmin";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Vendor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Vendor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DocumentNum">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DocumentNum">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DocumentNum">Delete</a> 
            </td>
        </tr>
}
    </tbody>
</table>
